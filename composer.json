{
    "name": "lily-labs/db-backup",
    "description": "DBBackup it's a library that provides classes to execute mysqldump to backup the given database, relocate the backup file in the local filesystem and compress the resulting backup file.",
    "keywords": [
        "LilyLabs",
        "DBBackup",
        "PHP MySQl Backup"
    ],
    "homepage": "https://github.com/spatie/DBBackup",
    "license": "MIT",
    "authors": [
        {
            "name": "Abraham Ch√°vez",
            "email": "mindacj@gmail.com",
            "homepage": "https://abrahamchavez.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/process": "2.8.*|3.0.*|3.1.*"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LilyLabs\\DBBackup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LilyLabs\\DBBackup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
